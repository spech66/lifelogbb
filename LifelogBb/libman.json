{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "library": "jquery@3.6.1",
      "destination": "wwwroot/lib/jquery/",
      "files": [
        "jquery.min.js",
        "jquery.js",
        "jquery.min.map"
      ]
    },
    {
      "library": "font-awesome@6.2.0",
      "destination": "wwwroot/lib/font-awesome/",
      "files": [
        "css/all.min.css",
        "webfonts/fa-brands-400.ttf",
        "webfonts/fa-brands-400.woff2",
        "webfonts/fa-regular-400.ttf",
        "webfonts/fa-regular-400.woff2",
        "webfonts/fa-solid-900.ttf",
        "webfonts/fa-solid-900.woff2",
        "webfonts/fa-v4compatibility.ttf",
        "webfonts/fa-v4compatibility.woff2"
      ]
    },
    {
      "library": "regression@1.4.0",
      "destination": "wwwroot/lib/regression/"
    },
    {
      "provider": "unpkg",
      "library": "easymde@2.18.0",
      "destination": "wwwroot/lib/easymde/",
      "files": [
        "dist/easymde.min.css",
        "dist/easymde.min.js"
      ]
    },
    {
      "provider": "unpkg",
      "library": "jquery-validation@1.19.5",
      "destination": "wwwroot/lib/jquery-validation/",
      "files": [
        "dist/jquery.validate.min.js",
        "dist/localization/messages_de.min.js",
        "dist/additional-methods.min.js"
      ]
    },
    {
      "library": "jquery-validation-unobtrusive@4.0.0",
      "destination": "wwwroot/lib/jquery-validation-unobtrusive/",
      "files": [
        "jquery.validate.unobtrusive.min.js"
      ]
    },
    {
      "library": "fabric.js@5.2.4",
      "destination": "wwwroot/lib/fabricjs/",
      "files": [
        "fabric.min.js"
      ]
    },
    {
      "provider": "jsdelivr",
      "library": "@tabler/core@1.0.0-beta17",
      "destination": "wwwroot/lib/tabler/core/",
      "files": [
        "dist/js/tabler.min.js",
        "dist/css/tabler.min.css",
        "dist/libs/bootstrap/dist/js/bootstrap.min.js",
        "dist/libs/apexcharts/dist/locales/de.json",
        "dist/libs/apexcharts/dist/locales/en.json",
        "dist/libs/apexcharts/dist/apexcharts.min.css",
        "dist/libs/apexcharts/dist/apexcharts.min.js"
      ]
    },
    {
      "library": "tagify@4.17.7",
      "destination": "wwwroot/lib/tagify/",
      "files": [
        "tagify.min.js",
        "tagify.min.css"
      ]
    },
    {
      "provider": "unpkg",
      "library": "rrule@2.7.2",
      "destination": "wwwroot/lib/rrule/",
      "files": [
        "dist/es5/rrule.min.js"
      ]
    }
  ]
}